  <step>
    <name>Extrae del Cubo Consumo Detallado el Mes Indicado</name>
    <type>TableInput</type>
    <description />
    <distribute>Y</distribute>
    <custom_distribution />
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name />
    </partitioning>
    <connection>Vertica</connection>
    <sql>SELECT ghc.dimentidad_id AS entidad_id,
    ghc.establecimienton1_id establecimienton1_id,
    ghc.establecimienton2_id establecimienton2_id,
    COALESCE(ghc.establecimienton3_id, -1) AS establecimienton3_id ,
    COALESCE(ghc.establecimienton4_id, -1) AS establecimienton4_id ,
    ghc.dimproducto_id AS producto_id,
    ghc.dimunidad_medida_id AS unidad_medida_id,
    gdp2.anio::integer AS anio,
    gdp2.mes_id::integer as mes, 
    abs(sum(ghc.cantidad * ghc.afectacion_inventario::numeric)) total
   FROM olap.gal_h_consumo ghc
     JOIN olap.gen_d_periodo gdp2 ON gdp2.id = ghc.dimfecha_homologada_id
  WHERE ghc.dimconsumo_cc_id = 1 
  and 
  (gdp2.anio=date_part('YEAR',to_date('${FECHA1}','DD-MM-YYYY'))::varchar(4) and gdp2.mes_id =date_part('MONTH',to_date('${FECHA1}','DD-MM-YYYY')))  or 
  (gdp2.anio=date_part('YEAR',to_date('${FECHA2}','DD-MM-YYYY'))::varchar(4) and gdp2.mes_id =date_part('MONTH',to_date('${FECHA2}','DD-MM-YYYY')))
  GROUP BY ghc.dimentidad_id, ghc.establecimienton1_id, ghc.establecimienton2_id, (COALESCE(ghc.establecimienton3_id, -1)), 
  (COALESCE(ghc.establecimienton4_id, -1)), ghc.dimproducto_id, ghc.dimunidad_medida_id, 
  gdp2.anio,gdp2.mes_id;</sql>
    <limit>0</limit>
